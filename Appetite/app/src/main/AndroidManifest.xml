<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.appetite.appetite" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".model.Inicio"
            android:label="@string/app_name" >
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".model.Reservacion"
            android:screenOrientation="portrait" />
        <activity
            android:name=".model.MenuOpciones"
            android:label="@string/title_activity_menu"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".model.Presentacion" />
        <activity android:name=".model.Ticket" />
        <activity
            android:name=".model.Pago"
            android:configChanges="orientation|screenSize"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name=".model.ConfimacionPago" />
        <activity
            android:name=".model.Asientos"
            android:screenOrientation="portrait" />
        <activity android:name=".model.ImageTest" />
        <activity android:name=".ejemplo" >
        </activity>
        <activity
            android:name=".model.AsientosModel"
            android:label="Asientos" >
            <intent-filter>
                <action android:name="android.intent.action.AsientosModel" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
